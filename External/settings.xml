<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
	<mirrors>
        <mirror>
            <id>hwcentral</id>
            <mirrorOf>central</mirrorOf>
            <url>http://wlg1.artifactory.cd-cloud-artifact.tools.huawei.com/artifactory/cbu-maven-public/</url>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
            <id>JDK1.8</id>
            <activation>
                <activeByDefault>true</activeByDefault>
                <jdk>1.8</jdk>
            </activation>
            <properties>
                <maven.compiler.source>1.8</maven.compiler.source>
                <maven.compiler.target>1.8</maven.compiler.target>
                <maven.compiler.compilerVersion>1.8</maven.compiler.compilerVersion>
            </properties>
        </profile>
        <profile>
            <id>cbu-maven-product</id>
            <repositories>
               <repository>
                    <id>cbu-2rd</id>
                    <name>cbu-2rd</name>
                    <url>http://wlg1.artifactory.cd-cloud-artifact.tools.huawei.com/artifactory/Product-Public-Service/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>cbu-2rd-release</id>
                    <name>cbu-2rd-release</name>
                    <url>http://wlg1.artifactory.cd-cloud-artifact.tools.huawei.com/artifactory/Product-CBUCommon-release/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>cbu-2rd-snapshot</id>
                    <name>cbu-2rd-snapshot</name>
                    <url>http://wlg1.artifactory.cd-cloud-artifact.tools.huawei.com/artifactory/Product-CBUCommon-snapshot/</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>hw-2rd</id>
                    <name>hw-2rd</name>
                    <url>http://wlg1.artifactory.cd-cloud-artifact.tools.huawei.com/artifactory/sz-maven-product/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>cbu-maven-proxy</id>
                    <name>cbu-maven-proxy</name>
                    <url>http://wlg1.artifactory.cd-cloud-artifact.tools.huawei.com/artifactory/cbu-maven-proxy/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>cbu-3rd-mirror</id>
                    <name>cbu-3rd-mirror</name>
                    <url>http://wlg1.artifactory.cd-cloud-artifact.tools.huawei.com/artifactory/Product-cbu-3rd-maven-release/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>hw-maven-plugin</id>
                    <name>hw-maven-plugin</name>
                    <url>http://wlg1.artifactory.cd-cloud-artifact.tools.huawei.com/artifactory/sz-maven-product/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </pluginRepository>

                <pluginRepository>
                    <id>central-maven-plugin</id>
                    <name>central-maven-plugin</name>
                    <url>http://wlg1.artifactory.cd-cloud-artifact.tools.huawei.com/artifactory/cbu-maven-proxy/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>

    <pluginGroups>
        <pluginGroup>com.github.spotbugs</pluginGroup>
    </pluginGroups>

    <activeProfiles>
        <activeProfile>cbu-maven-product</activeProfile>
        <activeProfile>JDK1.8</activeProfile>
    </activeProfiles>
</settings>